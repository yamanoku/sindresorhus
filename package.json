{
  "name": "yamanoku",
  "version": "0.4.0",
  "description": "yamanoku CLI",
  "license": "MIT",
  "repository": "yamanoku/yamanoku",
  "author": {
    "name": "Oyama -Michinoku- Okuto",
    "email": "0910yama@gmail.com",
    "url": "https://yamanoku.net"
  },
  "bin": {
    "sindresorhus": "src/cli.js"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "fmt": "prettier-eslint --write '**/*.{js,json,md}'",
    "test": "eslint src"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "src"
  ],
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "keywords": [
    "cli-app",
    "cli",
    "yamanoku"
  ],
  "dependencies": {
    "import-jsx": "3.0.0",
    "ink": "2.5.0",
    "ink-link": "1.0.0",
    "ink-select-input": "3.1.2",
    "meow": "5.0.0",
    "opn": "5.5.0",
    "react": "16.11.0",
    "term-img": "4.1.0",
    "terminal-image": "0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "eslint": "6.5.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-react": "7.16.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "prettier-eslint-cli": "5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
