{
  "name": "yamanoku",
  "version": "0.2.0",
  "description": "yamanoku CLI",
  "license": "MIT",
  "repository": "yamanoku/yamanoku",
  "author": {
    "name": "Oyama -Michinoku- Okuto",
    "email": "0910yama@gmail.com",
    "url": "https://yamanoku.net"
  },
  "bin": {
    "sindresorhus": "src/cli.js"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "fmt": "prettier-eslint --write '**/*.{js,json,md}'",
    "lint": "eslint src"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "src"
  ],
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "keywords": [
    "cli-app",
    "cli",
    "yamanoku"
  ],
  "dependencies": {
    "import-jsx": "1.3.2",
    "ink": "0.4.1",
    "ink-link": "1.0.0",
    "ink-select-input": "1.1.0",
    "meow": "5.0.0",
    "opn": "5.5.0",
    "term-img": "2.1.0",
    "terminal-image": "0.1.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "eslint": "4.19.1",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "lint-staged": "7.3.0",
    "prettier": "1.17.1",
    "prettier-eslint-cli": "4.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
