{
	"name": "yamanoku",
	"version": "0.0.1",
	"description": "Oyama Michinoku CLI",
	"license": "MIT",
	"repository": "yamanoku/yamanoku",
	"author": {
		"name": "Okuto -Michinoku- Oyama",
		"email": "0910yama@gmail.com",
		"url": "https://yamanoku.net"
	},
	"bin": {
		"sindresorhus": "source/cli.js"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"precommit": "lint-staged",
		"lint": "eslint source"
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"files": [
		"source"
	],
	"babel": {
		"plugins": [
			[
				"transform-react-jsx",
				{
					"pragma": "h"
				}
			]
		]
	},
	"keywords": [
		"cli-app",
		"cli",
		"sindresorhus",
		"unicorns"
	],
	"dependencies": {
		"import-jsx": "^1.3.0",
		"ink": "^0.4.1",
		"ink-select-input": "^1.0.0",
		"meow": "^5.0.0",
		"opn": "^5.3.0",
		"term-img": "^2.1.0",
		"terminal-image": "^0.1.0"
	},
	"devDependencies": {
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"eslint-config-xo-react": "^0.16.0",
		"eslint-plugin-react": "^7.7.0",
		"execa": "^0.10.0",
		"lint-staged": "^7.1.0"
	}
}
